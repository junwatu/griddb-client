{
  "name": "@griddb/client-lib",
  "version": "1.0.0",
  "description": "A TypeScript client library for GridDB Web API with full CRUD operations support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run clean && npm run build",
    "example": "ts-node examples/basic-usage.ts"
  },
  "keywords": [
    "griddb",
    "database",
    "nosql",
    "client",
    "typescript",
    "crud",
    "web-api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/griddb-client-lib.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/griddb-client-lib/issues"
  },
  "homepage": "https://github.com/yourusername/griddb-client-lib#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
